<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.sayantanbanerjee.guessthecelebrity">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@drawable/avengerspng"
            android:label="Guess the Avenger!"
            android:roundIcon="@drawable/avengerspng"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Winner">
        </activity>
        <activity android:name=".HomeScreen">
        </activity>
        <activity android:name=".Guess">
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>